	<parameter name="OrderID" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT `ShehaniRestaurant`.`OrderFoodItemDetail`.`FoodId`,
	`ShehaniRestaurant`.`OrderFoodItemDetail`.`BearId`,
	`ShehaniRestaurant`.`OrderFoodItemDetail`.`Quantity`,
	`ShehaniRestaurant`.`OrderFoodItemDetail`.`UnitPrice`,
	`ShehaniRestaurant`.`Orders`.`OrderId`,
	`ShehaniRestaurant`.`Orders`.`CustomerId`,
	`ShehaniRestaurant`.`Orders`.`OrderDate`,
	`ShehaniRestaurant`.`Orders`.`TimePlace`,
	`ShehaniRestaurant`.`Orders`.`PaymentAmount`,
	`ShehaniRestaurant`.`Orders`.`DeliveryId`,
	`ShehaniRestaurant`.`OrderFoodItemDetail`.`OrderId`,
	`ShehaniRestaurant`.`NonAlcoholFoodItem`.`FoodId` AS ItemId,
	`ShehaniRestaurant`.`NonAlcoholFoodItem`.`Name` AS ItemName,
	`ShehaniRestaurant`.`NonAlcoholFoodItem`.`Description` ,
	`ShehaniRestaurant`.`NonAlcoholFoodItem`.`Price`,
	`ShehaniRestaurant`.`NonAlcoholFoodItem`.`AvailabilityQty`,
	`ShehaniRestaurant`.`NonAlcoholFoodItem`.`Active`
FROM `ShehaniRestaurant`.`Orders`
	Inner Join `ShehaniRestaurant`.`OrderFoodItemDetail` ON 
	 `ShehaniRestaurant`.`Orders`.`OrderId` = `ShehaniRestaurant`.`OrderFoodItemDetail`.`OrderId` 
	inner Join `ShehaniRestaurant`.`NonAlcoholFoodItem` ON 
	 `ShehaniRestaurant`.`OrderFoodItemDetail`.`FoodId` = `ShehaniRestaurant`.`NonAlcoholFoodItem`.`FoodId` 
WHERE 
	 `ShehaniRestaurant`.`Orders`.`OrderId` = $P{OrderID} 
UNION ALL 
SELECT `ShehaniRestaurant`.`OrderFoodItemDetail`.`FoodId`,
	`ShehaniRestaurant`.`OrderFoodItemDetail`.`BearId`,
	`ShehaniRestaurant`.`OrderFoodItemDetail`.`Quantity`,
	`ShehaniRestaurant`.`OrderFoodItemDetail`.`UnitPrice`,
	`ShehaniRestaurant`.`Orders`.`OrderId`,
	`ShehaniRestaurant`.`Orders`.`CustomerId`,
	`ShehaniRestaurant`.`Orders`.`OrderDate`,
	`ShehaniRestaurant`.`Orders`.`TimePlace`,
	`ShehaniRestaurant`.`Orders`.`PaymentAmount`,
	`ShehaniRestaurant`.`Orders`.`DeliveryId`,
	`ShehaniRestaurant`.`OrderFoodItemDetail`.`OrderId`,
	`ShehaniRestaurant`.`AlcoholFoodItem`.`BearId` AS ItemId,
	`ShehaniRestaurant`.`AlcoholFoodItem`.`Name` AS ItemName,
	`ShehaniRestaurant`.`AlcoholFoodItem`.`Price`,
	`ShehaniRestaurant`.`AlcoholFoodItem`.`Availability`,
	`ShehaniRestaurant`.`AlcoholFoodItem`.`AlcoholContent` ,
	`ShehaniRestaurant`.`AlcoholFoodItem`.`AvailableQty`
FROM `ShehaniRestaurant`.`Orders`
	Inner Join `ShehaniRestaurant`.`OrderFoodItemDetail` ON 
	 `ShehaniRestaurant`.`Orders`.`OrderId` = `ShehaniRestaurant`.`OrderFoodItemDetail`.`OrderId` 
	inner join `ShehaniRestaurant`.`AlcoholFoodItem` ON 
	 `ShehaniRestaurant`.`OrderFoodItemDetail`.`BearId` = `ShehaniRestaurant`.`AlcoholFoodItem`.`BearId` 
WHERE 
	 `ShehaniRestaurant`.`Orders`.`OrderId` = $P{OrderID}]]>
	</queryString>
